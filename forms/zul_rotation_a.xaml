<Window x:Class="ZulrahRotations.forms.zul_rotation_a"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZulrahRotations.forms"
        mc:Ignorable="d"
        Title="Zulrah Rotation A" 
        Height="Auto" Width="Auto">

    <Border Padding="5">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
            
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}">
                <Image>
                    <Image.Source>
                        <BitmapImage UriSource="..\zul_phase\zulrah_a.png" />
                    </Image.Source>
                </Image>
            </Button>
        </Grid>
    </Border>
</Window>
